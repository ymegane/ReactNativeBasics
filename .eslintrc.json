{
    "parser": "babel-eslint",
    "env": {
        "react-native/react-native": true
    },
    "extends": [
        "eslint:recommended", 
        "plugin:react/recommended"
     ],
     "parserOptions": {
         "ecmaFeatures": {
             "experimentalObjectRestSpread": true,
             "jsx": true
         },
         "sourceType": "module"
     },
     "plugins": [
         "react",
         "react-native"
     ]
}